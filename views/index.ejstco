            display: flex;
            gap: 1rem;
        }

        .hero-image {
            flex: 1;
        }

        .hero-image img {
            width: 100%;
            border-radius: 24px;
            box-shadow: 0 25px 50px -12px rgba(11,43,79,0.25);
        }

        /* ===== OFFER BANNER ===== */
